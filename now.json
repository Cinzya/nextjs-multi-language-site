{
  "version": 2,
  "name": "prismic-nextjs-website",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    { "src": "/images/.+", "headers": { "cache-control": "s-maxage=604800" } },
    { "src": "/preview\\?(?<query>.*)", "dest": "/preview?query=$query" },
    { "src": "/(?!preview)(?<locale>.*[^/]+)$", "dest": "/?locale=$1" },
    { "src": "/(?<locale>[^/]+)$(?<uid>[^/]+)$", "dest": "/?locale=$locale/page?uid=$uid" }
  ]
}